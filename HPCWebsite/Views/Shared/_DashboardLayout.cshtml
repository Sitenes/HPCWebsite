    <script>
        function loadCartItems() {
            fetch('/cart/getcartitems')
                .then(response => response.json())
                .then(data => {
                    if (!data.success) return;

                    const itemsContainer = document.getElementById('cart-items');
                    itemsContainer.innerHTML = '';
                    document.getElementById('cart-count').innerText = data.items.length;
                    document.getElementById('cart-total').innerText = 'مجموع: ' + formatCurrency(data.total) + ' تومان';

                    data.items.forEach(item => {
                        const html = `
                        <div class="dropdown-item d-flex p-4">
                            <a href="/cart" class="open-file"></a>
                            <span class="avatar avatar-xl br-5 me-3 align-self-center cover-image"
                                  style="background-image: url('${item.imageUrl}'); background-size: cover;"></span>
                            <div class="wd-50p">
                                <h5 class="mb-1">${item.serverName}</h5>
                                <p class="fs-13 text-muted mb-0">مدت اجاره: ${item.rentalDays} روز</p>
                                <p class="fs-13 text-muted mb-0">قیمت روزانه: ${formatCurrency(item.dailyPrice)} تومان</p>
                            </div>
                            <div class="ms-auto text-end d-flex">
                                <span class="fs-16 text-dark d-none d-sm-block px-4">
                                    ${formatCurrency(item.totalPrice)} تومان
                                </span>
                                <a href="javascript:void(0)" onclick="removeFromCart(${item.id})" class="fs-16 btn p-0 cart-trash">
                                    <i class="fe fe-trash-2 border text-danger brround d-block p-2"></i>
                                </a>
                            </div>
                        </div>`;
                        itemsContainer.insertAdjacentHTML('beforeend', html);
                    });
                });
        }

        function removeFromCart(itemId) {
            fetch('/cart/remove/' + itemId, { method: 'POST' })
                .then(() => loadCartItems());
        }

        function formatCurrency(number) {
            return number.toFixed(0).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        }
    </script>




